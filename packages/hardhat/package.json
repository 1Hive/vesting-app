    "qrcode-terminal": "^0.12.0",
    "ramda": "^0.27.1"
  },
  "scripts": {
    "chain": "hardhat node --hostname 0.0.0.0 --export-all ../react-app/src/contracts/hardhat_contracts.json",
    "fork": "hardhat node --fork https://xdai-archive.blockscout.com --export-all ../react-app/src/contracts/hardhat_contracts.json",
    "test": "hardhat test --network hardhat",
    "typechain": "typechain --target=ethers-v5 --out-dir typechain-types --glob '../subgraph/abis/*.json'",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy --export-all ../react-app/src/contracts/hardhat_contracts.json",
    "postdeploy": "hardhat run scripts/publish.js",
    "watch": "node scripts/watch.js",
    "accounts": "hardhat accounts",
    "balance": "hardhat balance",
    "send": "hardhat send",
    "generate": "hardhat generate",
    "account": "hardhat account",
    "verify": "hardhat etherscan-verify --api-key PSW8C433Q667DVEX5BCRMGNAH9FSGFZ7Q8"
  }
